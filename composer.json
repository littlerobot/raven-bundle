{
    "name": "misd/raven-bundle",
    "type": "symfony-bundle",
    "description": "Adds Raven authentication to your Symfony2 application",
    "keywords": ["authentication", "bundle", "University of Cambridge"],
    "homepage": "https://github.com/misd-service-development/raven-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Wilkinson",
            "email": "chris.wilkinson@admin.cam.ac.uk"
        }
    ],
    "support": {
        "issues": "https://github.com/misd-service-development/raven-bundle/issues"
    },
    "require": {
        "php": ">=5.3.3",
        "ext-openssl": "*",
        "symfony/framework-bundle": "~2.3",
        "symfony/security-bundle": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/browser-kit": "~2.3",
        "symfony/css-selector": "~2.3",
        "symfony/dependency-injection": ">2.3.26",
        "symfony/dom-crawler": "~2.3",
        "symfony/phpunit-bridge": "^3.0",
        "symfony/yaml": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Misd\\RavenBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
